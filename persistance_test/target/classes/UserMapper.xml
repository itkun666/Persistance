<mapper namespace="User">
    <!--sql的唯一标示 statementId-->
    <select id="selectList" returnType="com.itkun.pojo.User">
        select *　from user
    </select>


    <select id="selectOne" returnType="com.itkun.pojo.User" paramType="com.itkun.pojo.User">
        select *　from user where id=#{id} and userName=#{userName}
    </select>
    
</mapper>